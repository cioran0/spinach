{
    "manifest_version": 3,
    "name": "Spinach",
    "version": "1.0",
    "description": "Forces text to green and background to white, preserving images and specific site layouts.",
    "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["content-script.css"],
        "run_at": "document_start"
      }
    ]
  }